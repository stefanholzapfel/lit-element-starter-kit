{
  "name": "lit-element-starter-kit",
  "version": "1.0.1",
  "description": "A starter kit for lit-element apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/stefanholzapfel/lit-element-starter-kit.git"
  },
  "main": "index.ts",
  "scripts": {
    "start": "webpack-dev-server --mode development --host 0.0.0.0 --disable-host-check",
    "start-prod": "webpack-dev-server --mode production --host 0.0.0.0 --disable-host-check",
    "build-dev": "cross-env NODE_ENV=development webpack --mode development",
    "build-prod": "cross-env NODE_ENV=production webpack --mode production"
  },
  "author": "Stefan Holzapfel",
  "license": "MIT",
  "dependencies": {
    "@polymer/paper-spinner": "^3.0.2",
