    "postcss-loader": "^3.0.0",
    "sass-loader": "^9.0.3",
    "terser-webpack-plugin": "^4.0.0",
    "ts-essentials": "^7.0.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1",
