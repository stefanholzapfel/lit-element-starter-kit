    "@webcomponents/webcomponentsjs": "^2.4.4",
    "lit-element": "^2.3.1",
    "lit-state": "https://github.com/stefanholzapfel/lit-state.git",
    "lit-translate": "^1.2.1",
    "pwa-helpers": "^0.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.0",
